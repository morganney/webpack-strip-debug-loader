{
  "name": "webpack-strip-debug-loader",
  "version": "1.3.4",
  "description": "Strips debug imports, declarations and expressions from your webpack bundles",
  "main": "dist",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/morganney/webpack-strip-debug-loader.git"
  },
  "bugs": {
    "url": "https://github.com/morganney/webpack-strip-debug-loader/issues"
  },
  "engines": {
    "node": ">=20.10.0",
    "npm": ">=9.8.0"
  },
  "scripts": {
    "prepack": "node --experimental-json-modules ./prepack.js",
    "lint": "eslint . src __tests__ --ext .js,.cjs",
    "lint:fix": "npm run lint -- --fix",
    "test": "node --experimental-vm-modules ./node_modules/.bin/jest"
  },
  "keywords": [
    "strip",
    "remove",
    "debug",
    "webpack",
    "loader"
  ],
  "author": "Morgan Ney <morganney@gmail.com>",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "debug": "^4.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.7",
    "@babel/eslint-parser": "^7.23.3",
    "@babel/plugin-syntax-import-attributes": "^7.23.3",
    "@babel/preset-env": "^7.23.8",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^27.6.3",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "memfs": "^4.6.0",
    "prettier": "^3.2.4",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  }
}
